    <p data-bind="text: trObs('@Model.InfoText')"></p>
    <div class="img-wrapper">
        <img src="@Url.WebObjectPath("~/Content/Images/Form/thank-you.png")" alt="Thank You" style="cursor: pointer" onclick="location.reload();" />
    </div>
</div>